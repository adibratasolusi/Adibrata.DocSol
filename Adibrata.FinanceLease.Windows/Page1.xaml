      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:Prefs="clr-namespace:Adibrata.Framework.Logging;assembly=Adibrata.Framework.Logging"
      xmlns:UserControler="clr-namespace:Adibrata.Windows.UserControler;assembly=Adibrata.Windows.UserControler" x:Class="Adibrata.FinanceLease.Windows.Page1"
      mc:Ignorable="d" 
      d:DesignHeight="1024"
      Title="Page Test" Width="1435">
    <Page.Resources>
        <Style x:Key="GridStyle1" TargetType="{x:Type Grid}"/>
    </Page.Resources>
    <Grid Margin="0,0,0,0" Style="{DynamicResource GridStyle1}">
        <Button Content="Button" HorizontalAlignment="Left" Margin="155,120,0,0" VerticalAlignment="Top" Width="75" Name="btnTest" Click="btnTest_Click"/>
        <WebBrowser HorizontalAlignment="Left" Height="100" Margin="80,195,0,0" VerticalAlignment="Top" Width="435" Name="winBrowser"/>

    </Grid>
</Page>
